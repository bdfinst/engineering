{
  "name": "engineering.dso.mil",
  "version": "0.1.0",
  "private": true,
  "description": "Recommended practices for Platform One",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run clean && npm run develop",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths",
    "clean": "rimraf .cache public",
    "serve": "gatsby serve",
    "now-build": "npm run build",
    "format": "npm run format:fix && npm run lint:js",
    "format:fix": "prettier --write '**/*.{js,jsx,yml,json,css,md}'",
    "lint:js": "eslint --cache --ext .js,.jsx . --fix",
    "lint": "concurrently 'npm run lint:js' 'npm run format:fix'",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "absolute",
      "options": {}
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "clsx": "^1.1.1",
    "formik": "^2.2.6",
    "gatsby": "^3.2.1",
    "gatsby-plugin-catch-links": "^3.2.0",
    "gatsby-plugin-image": "^1.2.0",
    "gatsby-plugin-local-search": "^2.0.0",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-material-ui": "^3.0.0",
    "gatsby-plugin-offline": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-remark-autolink-headers": "^3.2.0",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-use-flexsearch": "^0.1.1",
    "styled-components": "^5.2.3",
    "typeface-roboto": "^1.1.13",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.0.0",
    "dictionary-en-us": "^3.0.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-watch": "^7.0.0",
    "husky": "^6.0.0",
    "import-sort-style-absolute": "^1.0.1",
    "prettier": "^1.18.2",
    "remark-frontmatter": "^3.0.0",
    "remark-preset-lint-consistent": "^4.0.0",
    "remark-preset-lint-markdown-style-guide": "^4.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "remark-retext": "^3.1.3",
    "retext-english": "^3.0.4",
    "retext-repeated-words": "^3.0.0",
    "retext-sentence-spacing": "^4.0.0",
    "retext-spell": "^4.0.0",
    "retext-syntax-urls": "^2.0.0",
    "retext-usage": "^0.5.0",
    "rimraf": "^3.0.2",
    "unified": "^8.4.2"
  }
}
